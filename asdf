    <script src="js/jquery.dlmenu.js"></script>
    
	<script>
	//jQuery.noConflict();
$(document).ready(function(){
$("#requestfom").on('submit',function(e){
 e.preventDefault();
 
	var name = $("#name").val();
	var mail= $("#mail").val();
	var mobile = $("#mobile").val();
	var ppc = $("#ppc").val();
	var budget = $("#budget").val();
	var budgetpro = $("#budgetpro").val();
	if($.isNumeric(mobile)){
	
 $.ajax({
        type:"POST",
        url:"requestmail.php",
        data:"namee=" + name+ "&mail=" + mail+ "&mobile=" + mobile+"&ppc="+ppc+ "&budget="+budget+"&budgetpro="+budgetpro,
        success: function(response){
		var name = $("#name").val("");
	$("#requestfom").hide();
	
	
		$("#res").html(response);
		
		e.preventDefault();
		$("#phonvalidation").html("");
		return false;
		//$("#requestid").trigger("click");
            //alert(response); 
        }		
    });
	}
	else
	{
	$("#phonvalidation").html("please fill valid phone number");
	
	}	
});
	$('#quote_popup').on('hidden.bs.modal', function (e) {
		$("#requestfom").show();
	$("#res").text("");
	})
});
  
    </script>
<script>
        $(function() {
            $('#dl-menu').dlmenu();
        });
    </script>
